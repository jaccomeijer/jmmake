{
  "name": "@wheelroom/make",
  "version": "6.20.1",
  "description": "Make library for use with NPM 7 and @npmcli/arborist ",
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:module": "rollup --config",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch"
  },
  "dependencies": {
    "@npmcli/arborist": "2.3.0",
    "@octokit/rest": "18.5.3",
    "conventional-changelog": "3.1.24",
    "conventional-recommended-bump": "6.1.0",
    "deepmerge": "4.2.2",
    "inquirer": "8.0.0",
    "semver": "7.3.5",
    "standard-version": "9.3.0",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@types/standard-version": "7.0.0"
  }
}
