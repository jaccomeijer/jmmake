{
  "name": "@wheelroom/make",
  "version": "6.24.1",
  "description": "Make library for use with NPM 7 and @npmcli/arborist ",
  "exports": {
    "./*": "./build/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "scripts": {
    "build": "npm-run-all clean build:* copy-templates",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:compile": "rollup --config",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.packages.json --declaration --emitDeclarationOnly --declarationDir build",
    "copy-templates": "cp -r src/commands/templates build/commands",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch"
  },
  "dependencies": {
    "@npmcli/arborist": "2.3.0",
    "@octokit/rest": "18.5.3",
    "conventional-changelog": "3.1.24",
    "conventional-changelog-conventionalcommits": "4.6.0",
    "conventional-recommended-bump": "6.1.0",
    "deepmerge": "4.2.2",
    "inquirer": "8.0.0",
    "semver": "7.3.5",
    "yargs": "17.0.1"
  }
}